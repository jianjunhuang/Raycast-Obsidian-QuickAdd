{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "obsidian-quick-add",
  "title": "Obsidian Quick Add",
  "description": "Add Obsidian Note or Task",
  "icon": "command-icon.png",
  "author": "juncat",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "quick-add-task",
      "title": "Quick Add Task",
      "description": "Add Obsidian Task",
      "mode": "view"
    },
    {
      "name": "quick-add-later",
      "title": "Quick Add Later",
      "description": "Add Obsidian Later",
      "mode": "view"
    },
    {
      "name": "quick-add-list",
      "title": "Quick Add List",
      "description": "Add Obsidian Content",
      "mode": "view"
    },
    {
      "name": "quick-add-index",
      "title": "Quick Add Search",
      "description": "Add Obsidian Content",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.3",
    "@raycast/utils": "^1.18.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
